<p id="notice"><%= notice %></p>

<h1>Listing Finds</h1>

<table>
  <thead>
    <tr>
      <th>Album</th>
      <th>User</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @finds.each do |find| %>
      <tr>
        <td><%= find.album_id %></td>
        <td><%= find.user_id %></td>
        <td><%= find.photo %></td>
        <td><%= link_to 'Show', find %></td>
        <td><%= link_to 'Edit', edit_find_path(find) %></td>
        <td><%= link_to 'Destroy', find, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Find', new_find_path %>
